}\n\n    function handleClick(event) {\n        event.preventDefault()\n        axios.get(`https://api.weather.gov/points/${coordinates.latitude},${coordinates.longitude}`)\n            .then(response => {\n                setXcoord(response.data.properties.gridX)\n                setYcoord(response.data.properties.gridY)\n                console.log(xCoord, yCoord)\n                coordinates.latitude = \"\"\n                coordinates.longitude = \"\"\n\n            })\n            .catch(error => console.log(error))\n\n    }\n\n    useEffect(() => {\n        axios.get(`https://api.weather.gov/gridpoints/LWX/${xCoord},${yCoord}/forecast`)\n            // .then(response => console.log(response.data.properties.periods[0].temperature))\n            .then(response => {\n                setTemperature(response.data.properties.periods[0].temperature)\n                setShortForecast(response.data.properties.periods[0].shortForecast)\n                setWindSpeed(response.data.properties.periods[0].windSpeed)\n                setWindDirection(response.data.properties.periods[0].windDirection)\n\n            })\n            .catch(error => console.log(error))\n    }, [xCoord, yCoord])\n\n\n\n    return (\n\n        <ThemeContext.Provider value={{\n            coordinates: coordinates.latitude,\n            coordinates: coordinates.longitude,\n            handleChange,\n            handleClick,\n            temperature,\n            shortForecast,\n            windDirection,\n            windSpeed\n\n        }}>\n            {props.children}\n\n        </ThemeContext.Provider>\n\n    )\n}\n\nexport { ThemeContext, ThemeContextProvider }","/Users/chrismiller/development/vschool/assignments/level-4-capstone/react-api-project/src/Wind.js",[],[],{"ruleId":"41","severity":1,"message":"42","line":63,"column":13,"nodeType":"43","messageId":"44","endLine":63,"endColumn":24},"no-dupe-keys","Duplicate key 'coordinates'.","ObjectExpression","unexpected"]